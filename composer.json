{
    "name": "tenolo/twig-extensions-bundle",
    "description": "basic twig extensions for symfony.",
    "keywords": [
        "base",
        "twig",
        "extension",
        "extensions",
        "symfony",
        "bundle",
        "tenolo"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "homepage": "http://tenolo.de",
    "authors": [
        {
            "name": "tenolo - Robert Bernis, Johannes Herberhold GbR",
            "email": "info@tenolo.de",
            "homepage": "http://tenolo.de"
        },
        {
            "name": "Nikita Loges",
            "email": "n.loges@tenolo.de"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",

        "symfony/framework-bundle": "~3.4|~4.0",
        "symfony/config": "~3.4|~4.0",
        "symfony/dependency-injection": "~3.4|~4.0",
        "symfony/http-kernel": "~3.4|~4.0",
        "symfony/http-foundation": "~3.4|~4.0",
        "symfony/routing": "~3.4|~4.0",

        "tenolo/twig-extensions": "~0.3|~1.0",

        "phive/twig-extensions-deferred": "~1.0|~2.0",
        "twig/extensions": "~1.5",
        "twig/twig": "~1.34|~2.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.tenolo.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tenolo\\Bundle\\TwigExtensionsBundle\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
